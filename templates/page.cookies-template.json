        },
        "subheading_YRgXxk": {
          "type": "subheading",
          "settings": {
            "subheading": "For Safari:",
            "subheading_tag": "h3",
            "subheading_font_class": "fs-heading-6-base"
          }
        },
        "text_GdDdxF": {
          "type": "text",
          "settings": {
            "text": "<p>1. Choose Preferences &gt; Privacy<br/>2. Click on “Block all cookies”</p>",
            "text_font_class": "fs-body-75"
